package easy;

public class multiof5_3 {
	public static void main(String[] args){
		int test = 1000;
		System.out.println(multi(test));
	}
	
	public static long multi(int n){
		long count = 1;
		int i = 1;
		while(i <= 1000)){
			if((i % 3) == 0 && (i % 5) == 0){
				count = count * i;
				
			}
			else if(i % 3 == 0) count = count * i;
			else if(i % 5 == 0) count = count * i;
			
			i ++;
			
		}
		return count;
		
	}
}
