package easy;

public class multiof5_3 {
	public static void main(String[] args){
		int test = 1000;
		System.out.println(multi(500));
	}
	
	public static long multi(int n){
		long count = 2;
		for(long i = 3; i <= 500; i++){
			if((i % 3) == 0 || (i % 5) == 0){
				count = count * i;
				System.out.println(count);
			}
			
		}
		return count;
		
	}
}
